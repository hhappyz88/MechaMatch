{
  "site": "aztoyhobby",
  "start_urls": ["https://aztoyhobby.com/collections/all"],
  "pagination_strategy": {
    "type": "next_button",
    "selector": "a.pagination__item--prev"
  },
  "product_strategy": {
    "type": "link_extractor",
    "allow": ["/products/[\\w\\-]+$"],
    "deny": []
  },
  "image_selectors": ["div.t4s-product__media img"],
  "image_attrs": ["srcset"]
}