{
  "site": "g2bgundamstore.json",
  "start_urls": [
    "https://g2bgundamstore.com/collections/all"
  ],
  "pagination_strategy": {
    "type": "next_button",
    "selector": "a.pagination__item--next"
  },
  "product_strategy": {
    "type": "link_extractor",
    "allow": [
      "/products/[\\w\\-]+$"
    ],
    "deny": []
  },
  "image_selectors": [
    "div.product__media img"
  ],
  "image_attrs": [
    "src"
  ]
}